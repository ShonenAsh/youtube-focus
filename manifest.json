{
  "manifest_version": 3,
  "name": "Youtube Focus Mode",
  "version": "0.1.0",
  "action": {
    "default_popup": "src/index.html",
    "default_icon": "favicon.png"
  },
  "content_scripts" : [
    {
      "js" : ["src/content.js"], 
      "matches" : [
        "https://www.youtube.com/*"
      ]
    }
  ]
}